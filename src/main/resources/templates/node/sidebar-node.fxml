<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root fx:id="root"  xmlns="http://javafx.com/javafx/19"
         xmlns:fx="http://javafx.com/fxml/1" type="javafx.scene.layout.AnchorPane">
    <children>
        <VBox alignment="CENTER" spacing="10.0">
            <children>
                <Label text="Movie Stalker">
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="Arial Bold" size="20.0"/>
                    </font>
                </Label>
                <Button focusTraversable="false" mnemonicParsing="false" onAction="#onHome" prefHeight="30.0"
                        prefWidth="160.0" text="Home"/>
                <Button disable="true" focusTraversable="false" layoutX="30.0" layoutY="73.0" mnemonicParsing="false"
                        onAction="#onHome" prefHeight="30.0" prefWidth="160.0" text="Discover"/>
                <Button focusTraversable="false" mnemonicParsing="false" onAction="#onLibrary" prefHeight="30.0"
                        prefWidth="160.0" text="Library"/>
                <Button disable="true" focusTraversable="false" mnemonicParsing="false" prefHeight="30.0"
                        prefWidth="160.0" text="Watch"/>
                <Region prefHeight="483.0"/>
                <Button disable="true" focusTraversable="false" layoutX="30.0" layoutY="147.0" mnemonicParsing="false"
                        prefHeight="30.0" prefWidth="160.0" text="Settings"/>
                <Button focusTraversable="false" mnemonicParsing="false" onAction="#onLogout" prefHeight="30.0"
                        prefWidth="160.0" text="Log out" textFill="RED"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</fx:root>