<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>

<fx:root fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
         prefHeight="158.0" prefWidth="312.0" stylesheets="@../style/category-node-style.css" type="StackPane"
         xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Rectangle fx:id="categoryImage" arcHeight="10.0" arcWidth="10.0" fill="#c4c4c4" height="158.0" stroke="BLACK"
                   strokeType="INSIDE" strokeWidth="0.0" width="312.0"/>
        <HBox fx:id="categoryPane" onMouseEntered="#onCategory" onMouseExited="#onCategory" alignment="CENTER"
              prefHeight="158.0" prefWidth="312.0" spacing="12.0">
            <children>
                <Label id="category-label" fx:id="categoryLabel" text="Category"/>
            </children>
        </HBox>
    </children>
</fx:root>
